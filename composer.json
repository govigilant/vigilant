{
    "name": "laravel/laravel",
    "type": "project",
    "description": "Vigilant: All-in-one website monitoring",
    "keywords": [
        "monitoring",
        "laravel",
        "vigilant"
    ],
    "license": "AGPL-3.0",
    "require": {
        "php": "^8.2",
        "ext-dom": "*",
        "blade-ui-kit/blade-heroicons": "^2.3",
        "bluelibraries/dns": "@dev",
        "codeat3/blade-line-awesome-icons": "*",
        "codeat3/blade-phosphor-icons": "^2.2",
        "codeat3/blade-teeny-icons": "^1.9",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^11.0",
        "laravel/horizon": "^5.23",
        "laravel/jetstream": "^5.0",
        "laravel/octane": "^2.3",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.8",
        "livewire/livewire": "^3.0",
        "ramonrietdijk/livewire-tables": "^4.0",
        "vigilant/core": "@dev",
        "vigilant/crawler": "@dev",
        "vigilant/dns": "@dev",
        "vigilant/frontend": "@dev",
        "vigilant/lighthouse": "dev-main",
        "vigilant/notifications": "@dev",
        "vigilant/onboarding": "@dev",
        "vigilant/settings": "dev-main",
        "vigilant/sites": "@dev",
        "vigilant/uptime": "@dev",
        "vigilant/users": "@dev"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/dusk": "^8.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-install-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "./packages/*"
        },
        {
            "type": "vcs",
            "url": "git@github.com:VincentBean/dns.git"
        }
    ]
}
